{
    "name": "sam-it/symfony-http-psr18",
    "description": "Provides a Symfony HttpClientInterface that wraps PSR17 and PSR18",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SamIt\\SymfonyHttpPsr18\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sam Mousa",
            "email": "sam@mousa.nl"
        }
    ],
    "require": {
	"php": ">= 8.1",
    "psr/http-message": "^2",
    "psr/http-factory": "^1.0",
    "symfony/http-client-contracts": "^3",
    "psr/http-client": "^1.0"
    },
    "require-dev": {
        "vimeo/psalm": "^5",
        "phpstan/phpstan": "^1.8",
        "symplify/easy-coding-standard": "^12",
        "captainhook/plugin-composer": "^5.3",
        "ramsey/conventional-commits": "^1.3"
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true
        }
    }
}
